algoritmo "leia_nome"
// Função :
// Autor :
// Data : 19/10/2022
// Seção de Declarações 
var
   nome:caractere
inicio
// Seção de Comandos
   escreval("Qual é o seu nome? ")
   leia(nome)
   escreval("Olá ",nome," ,é um prazer te conhecer! ")
   escreval("  ")
   escreval(" FIM DO PROGRAMA ")
fimalgoritmo
