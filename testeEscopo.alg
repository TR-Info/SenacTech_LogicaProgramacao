algoritmo "testeescopo"
// Função :
// Autor :
// Data : 24/10/2022
// Seção de Declarações 
var
   n1,n2:inteiro
procedimento rotina(a,b:inteiro)
var
   x,y:inteiro
inicio
// Seção de Comandos
   x<-a
   y<-b
   escreva(n1,n2,a,b,x,y)
fimprocedimento
inicio
   n1<-9
   n2<-5
   rotina(n1,n2)


fimalgoritmo
